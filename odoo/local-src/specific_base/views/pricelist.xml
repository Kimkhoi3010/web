<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="product_pricelist_depiltech_form" model="ir.ui.view">
        <field name="name">product.pricelist.depiltech.form</field>
        <field name="model">product.pricelist</field>
        <field name="inherit_id" ref="product.product_pricelist_view"/>
        <field name="priority" eval="48"/>
        <field name="arch" type="xml">
            <field name="company_id" position="after">
                <label for="allowed_company_ids"/>
                <field name="allowed_company_ids" nolabel="1" widget="many2many_tags"/>
            </field>
        </field>
    </record>

</odoo>
