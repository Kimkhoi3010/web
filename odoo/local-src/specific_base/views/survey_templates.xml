<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

    <template id="inherit_survey_sfinished" inherit_id="survey.sfinished">
        <xpath expr="//t[@t-call='survey.back']" position="after">
            <t t-call="specific_base.back_partner" />
        </xpath>
    </template>


    <template id="back_partner" name="BackPartner">
        <div groups="base.group_website_publisher" t-ignore="true" class="pull-right">
            <a t-attf-href="/web#view_type=form&amp;model=res.partner&amp;id=#{user_input.partner_id.id}&amp;action=base.action_partner_form" class="btn btn-default">Back to Partner</a>
        </div>
    </template>


</data>
</openerp>
