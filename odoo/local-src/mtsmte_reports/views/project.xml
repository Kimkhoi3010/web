<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record model="ir.ui.view" id="project_print_view">
    <field name="name">mts project.project print buttons</field>
    <field name="model">project.project</field>
    <field name="inherit_id" ref="project.edit_project"/>
    <field name="arch" type="xml">
      <xpath expr="//sheet" position="before">
        <header>
          <button
            name="%(mtsmte_reports.act_project_analysis_wiz_form)d"
            type="action" string="Print analysis"
            />
        </header>
      </xpath>
    </field>
  </record>


</odoo>
